<section id="cal-container">
    <section id="cal-head">
        <span id="btn-today">today</span>
        <span id="btn-last">&lt</span>
        <span id="btn-next">&gt</span>
        <h1 id="cal-title"></h1>
    </section>
    <section id="cal">
    </section>
</section>
<section id="event-panel">
    {{> events_panel}}
</section>
<script>
    let event_list = [
        {{#each event_list}}
        {title: '{{this.title}}', deadline: new Date('{{this.deadline}}'), priority: {{this.priority}} },
        {{/each}} ]
</script>
<script src="public/js/cal_setup.js" defer></script>
